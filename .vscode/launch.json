	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Program",
			"program": "${workspaceFolder}/bin/www",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node",
			"preLaunchTask": "gulp: ci"
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach",
			"sourceMaps": true,
			"port": 9229,
			"remoteRoot": "/app",
			"smartStep": true,
			"restart": true,
			"skipFiles": [
				"<node_internals>/**"
			]
		}
	]
}