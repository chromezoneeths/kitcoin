{
	"name": "kitcoin",
	"version": "0.2.0",
	"license": "MIT",
	"scripts": {
		"start": "node ./bin/www",
		"build": "gulp",
		"ci": "gulp ci",
		"package": "gulp pkg",
		"test:internal": "mocha --exit",
		"test": "bash tests.sh"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cookie-parser": "~1.4.4",
		"cron": "^1.8.2",
		"debug": "~2.6.9",
		"ejs": "~2.6.1",
		"express": "~4.16.1",
		"express-ws": "^4.0.0",
		"googleapis": "^44.0.0",
		"http-errors": "~1.6.3",
		"mongodb": "^3.3.3",
		"morgan": "~1.9.1",
		"redis": "^3.0.2",
		"uuid": "^3.3.3",
		"ws": "^7.2.0"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.2",
		"@types/express": "^4.17.4",
		"@types/http-errors": "^1.6.3",
		"@types/mongodb": "^3.5.5",
		"@types/morgan": "^1.9.0",
		"@types/redis": "^2.8.20",
		"@types/uuid": "^7.0.2",
		"@types/ws": "^7.2.3",
		"chai": "^4.2.0",
		"del": "^5.1.0",
		"docker-compose": "^0.23.4",
		"fs-extra": "^9.0.1",
		"gulp": "^4.0.0",
		"gulp-copy": "^4.0.1",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^6.0.0-alpha.1",
		"gulp-xo": "^0.24.0",
		"jest-cli": "^25.3.0",
		"mocha": "^7.2.0",
		"pkg": "^4.4.8",
		"typescript": "^3.8.3",
		"xo": "^0.28.3"
	},
	"xo": {
		"rules": {
			"no-await-in-loop": 0,
			"new-cap": 0,
			"@typescript-eslint/restrict-template-expressions": 0,
			"unicorn/import-index": 0,
			"import/no-useless-path-segments": 0,
			"@typescript-eslint/no-var-requires": 0
		},
		"overrides": [
			{
				"files": "clientJs/*.js",
				"envs": [
					"browser"
				]
			},
			{
				"files": "test/*.test.js",
				"envs": [
					"mocha"
				]
			}
		]
	}
}